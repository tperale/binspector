{
  "verbose": true,
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "testEnvironment": "node",
  "testMatch": [
    "<rootDir>/src/**/__tests__/*.test.ts",
    "<rootDir>/example/**/*.test.ts"
  ],
  "transform": {
    "\\.ts$": "babel-jest"
  },
  "collectCoverage": true,
  "coverageDirectory": "./coverage/"
}
