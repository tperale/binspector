{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "./src/decorators/controller.ts",
    "./src/decorators/validator.ts",
    "./src/decorators/transformer.ts",
    "./src/decorators/condition.ts",
    "./src/decorators/prepost.ts",
    "./src/decorators/bitfield.ts",
    "./src/decorators/primitive.ts"
  ],
  "highlightLanguages": ["c", "typescript"],
  "out": "docs",
  "excludeExternals": true,
  "includeVersion": true,
  "exclude": ["**/__test__/*.test.ts*"],
  "plugin": ["typedoc-plugin-mermaid"],
  "projectDocuments": ["src/decorators/*.md"]
}
