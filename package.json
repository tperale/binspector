  "main": "dist/parser.js",
  "module": "src/index.ts",
  "scripts": {
    "build": "webpack --mode=development",
    "test": "jest",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "jsdoc": "jsdoc -a all -c jsdoc.json -r -R README.md index.js -d docs/jsdoc",
