{
  "name": "binspector",
  "description": "A _truly_ declarative library for binary file and protocol definition written in typescript. Read binary files based on class definition and decorators directly from your webapp.",
  "version": "1.1.0",
  "types": "module",
  "exports": "./dist/lib/index.js",
  "files": [
    "/dist/lib",
    "/src"
  ],
  "scripts": {
    "clean": "npx rimraf dist",
    "build": "npm run clean && tsc",
    "test": "node --warnings --experimental-vm-modules node_modules/.bin/jest",
    "lint": "eslint",
    "doc": "typedoc --options typedoc.json --mermaidVersion latest"
  },
  "author": "thomperale",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@eslint/js": "^9.14.0",
    "@stylistic/eslint-plugin": "^4.0.1",
    "@types/jest": "^29.5.14",
    "eslint": "^9.21.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.3",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.2",
    "typedoc": "^0.27.9",
    "typedoc-plugin-mermaid": "^1.12.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  },
  "directories": {
    "doc": "docs",
    "example": "example"
  },
  "homepage": "https://tperale.github.io/binspector/",
  "repository": {
    "type": "git",
    "url": "https://github.com/tperale/binspector.git"
  },
